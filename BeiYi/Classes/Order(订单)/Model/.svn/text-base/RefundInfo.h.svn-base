#import <Foundation/Foundation.h>

@interface RefundInfo : NSObject

// 退单原因
@property (nonatomic, copy) NSString *reason;
// 凭证时间
@property (nonatomic, copy) NSString *confirm_time;
// 退款价格
@property (nonatomic, copy) NSString *refund_price;
// 退款ID
@property (nonatomic, copy) NSString *refund_id;
// 退款类型
@property (nonatomic, copy) NSString *refund_type;
// 退单状态
@property (nonatomic, copy) NSString *refund_status;
// 请求时间
@property (nonatomic, copy) NSString *apply_time;

@end
